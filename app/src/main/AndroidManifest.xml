<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.qwwuyu.example">
    <!-- permitTest -->
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:name=".TApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:windowSoftInputMode="adjustResize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.ELMActivity"/>
        <activity android:name=".activity.GlideActivity"/>
        <activity android:name=".activity.HttpActivity"/>
        <activity android:name=".activity.ColorActivity"/>
        <activity android:name=".activity.GyroActivity"/>
        <activity android:name=".activity.TestActivity"/>
        <activity android:name=".activity.Test2Activity"/>
        <activity android:name=".activity.MvpActivity"/>
    </application>
</manifest>